{
  "name": "@openweight/sdk",
  "version": "0.3.0",
  "description": "TypeScript SDK for openweight - parse, validate, and serialize workout data",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate:schemas": "node scripts/generate-schemas.js",
    "build": "npm run generate:schemas && tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openweight",
    "fitness",
    "strength-training",
    "workout",
    "json-schema",
    "typescript"
  ],
  "author": "openweight contributors",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radupana/openweight.git",
    "directory": "packages/ts-sdk"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=18"
  }
}
